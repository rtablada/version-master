{
    "name": "rtablada/version-master",
    "description": "A git based versioning tool for PHP projects with Laravel Service Providers",
    "keywords": [
        "rtablada",
        "version-master"
    ],
    "homepage": "https://github.com/thephprtablada/version-master",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Tablada",
            "email": "ryan.tablada@gmail.com",
            "homepage": "http://ryantablada.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "illuminate/filesystem": "^5.1",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rtablada\\VersionMaster\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rtablada\\VersionMaster\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "archive": {
        "exclude": ["/dummy"]
    }
}
